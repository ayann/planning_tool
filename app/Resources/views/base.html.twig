<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}{% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />        
        
         <!---CSS Files-->

        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/layouts/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/layouts/css/bootstrap-theme.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/layouts/css/font-awesome.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/layouts/css/annimate.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/layouts/css/main.css') }}">

        <!---jQuery Files-->        
        <script src="{{ asset('bundles/layouts/js/jquery.min.js') }}"></script>
        
        <!---jQuery DataTable-->        
        <script src="{{ asset('bundles/datatable/media/js/jquery.js') }}"></script>
        <script src="{{ asset('bundles/datatable/media/js/jquery.dataTables.js') }}"></script>
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/datatable/media/css/jquery.dataTables.css') }}">
        
        <!-- Bootstrap JavaScript -->
        <script src="{{ asset('bundles/layouts/js/bootstrap.min.js') }}"></script>
        
        <!-- Application JavaScript -->
        <script src="{{ asset('bundles/layouts/js/main.js') }}"></script>
        
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        
    </head>
    <body>
        {% block body %}{% endblock %}
        
        {% block javascripts %}{% endblock %}
    </body>
</html>
