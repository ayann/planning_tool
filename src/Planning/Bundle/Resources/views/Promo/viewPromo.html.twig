{% extends 'PlanningBundle::layout.html.twig' %}

{% block main_content %}

    <table id="tabPromo" class="display" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>name</th>
                <th>number</th>
                <th>bouton</th>
            </tr>
        </thead>

        <tfoot>
            <tr>
                <th>name</th>
                <th>number</th>
                <th>bouton</th>
            </tr>
        </tfoot>
    </table>

{% endblock main_content %}

{% block javascripts %}
    {{ parent() }}

    <script>

        $(document).ready(function() {
            $('#tabPromo').dataTable( {
                "processing": true,
                "serverSide": true,
                "ajax": {
                    "url": "{{ path('planning_getdatapromo') }}",
                    "dataType": "jsonp"
                }
            } );
        } );

    </script> 



{% endblock %}
   